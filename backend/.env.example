# 服务器配置
PORT=3000
NODE_ENV=development

# 飞书应用配置
FEISHU_APP_ID=your_app_id_here
FEISHU_APP_SECRET=your_app_secret_here

# 飞书多维表格配置
# 项目表
FEISHU_PROJECT_APP_TOKEN=your_project_app_token
FEISHU_PROJECT_TABLE_ID=your_project_table_id

# Bug 表
FEISHU_BUG_APP_TOKEN=your_bug_app_token
FEISHU_BUG_TABLE_ID=your_bug_table_id

# 工单表
FEISHU_TICKET_APP_TOKEN=your_ticket_app_token
FEISHU_TICKET_TABLE_ID=your_ticket_table_id

# 成员表
FEISHU_MEMBER_APP_TOKEN=your_member_app_token
FEISHU_MEMBER_TABLE_ID=your_member_table_id

# 知识库表
FEISHU_KNOWLEDGE_APP_TOKEN=your_knowledge_app_token
FEISHU_KNOWLEDGE_TABLE_ID=your_knowledge_table_id

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 缓存配置（秒）
CACHE_TTL_SHORT=300      # 5分钟
CACHE_TTL_MEDIUM=1800    # 30分钟
CACHE_TTL_LONG=3600      # 1小时

# CORS 配置
CORS_ORIGIN=http://localhost:8000

# DeepSeek API 配置（用于 AI 功能）
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_URL=https://api.deepseek.com

